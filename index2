                    { url: "https://1drv.ms/i/s!Arw8scDaFkwGg61IhGY2nn_9ptIWig?embed=1?", tags: ["_TODO"] },
                    { url: "https://1drv.ms/i/s!Arw8scDaFkwGg61G1YUZLT9DkNmL9Q?embed=1?", tags: ["_TODO"] },
                    { url: "https://1drv.ms/i/s!Arw8scDaFkwGg60-SeXatohL51ElvQ?embed=1?", tags: ["_TODO"] },
                    { url: "https://1drv.ms/i/s!Arw8scDaFkwGg608K9eCUfrCplGanA?embed=1?", tags: ["_TODO"] },
                    { url: "https://1drv.ms/i/s!Arw8scDaFkwGg603NfvRNDVJOFFGcQ?embed=1?", tags: ["_TODO"] },
                    { url: "https://1drv.ms/i/s!Arw8scDaFkwGg60_jsXqVkVW2QIUOg?embed=1?", tags: ["_TODO"] },
                ]
            },

        ]

        const slideshowContainer = document.querySelector('.slideshow-container');
        const portfolioContainer = document.getElementById('portfolio-container');

        // Helper to normalize image entries into consistent objects
        function normalizeImage(item) {
            if (typeof item === 'string') {
                return { url: item, fallbackUrl: '', tags: [] };
            }
            return {
                url: item.url || '',
                fallbackUrl: item.fallbackUrl || '',
                tags: item.tags || []
            };
        }

        galleryData.forEach(gallery => {
            const block = document.createElement('div');
            block.className = 'slideshow-block';
            block.dataset.target = gallery.id;
            block.title = `${gallery.title} Gallery`;

            // Slideshow preview: show first image only (you can increase count if you want slideshow)
            gallery.images.slice(0, 1).forEach((item, i) => {
                const imgData = normalizeImage(item);
                const img = document.createElement('img');
                img.className = 'slide' + (i === 0 ? ' active' : '');
                img.alt = `${gallery.title} image ${i + 1}`;
                img.loading = 'lazy';

                img.src = imgData.url; // already sized URLs, no imageSize appended here

                block.appendChild(img);
            });

            // Overlay and label
            const overlay = document.createElement('div');
            overlay.className = 'overlay';
            overlay.textContent = gallery.title;
            block.appendChild(overlay);

            const label = document.createElement('div');
            label.className = 'section-label';
            label.textContent = gallery.title;
            block.appendChild(label);

            slideshowContainer.appendChild(block);

            // Portfolio section
            const section = document.createElement('section');
            section.id = gallery.id;
            section.className = 'portfolio-section';

            const title = document.createElement('h2');
            title.textContent = gallery.title;
            section.appendChild(title);

            const imageWrap = document.createElement('div');
            imageWrap.className = 'portfolio-images';

            gallery.images.forEach(item => {
                const imgData = normalizeImage(item);
                const img = document.createElement('img');
                img.dataset.base = imgData.url;
                img.src = imgData.url + imageSize; // append sizing here for portfolio
                img.alt = `${gallery.title}`;
                img.loading = 'lazy';

                // fallback if any
                if (imgData.fallbackUrl) {
                    img.onerror = () => {
                        img.onerror = null;
                        img.src = imgData.fallbackUrl;
                    };
                }

                imageWrap.appendChild(img);
            });

            section.appendChild(imageWrap);
            portfolioContainer.appendChild(section);
        });


        // Orientation detection for portfolio images
        function detectOrientation() {
            document.querySelectorAll('.portfolio-images img').forEach(img => {
                img.onload = () => {
                    if (img.naturalWidth < img.naturalHeight) {
                        img.classList.add('portrait');
                        img.classList.remove('landscape');
                    } else {
                        img.classList.add('landscape');
                        img.classList.remove('portrait');
                    }
                };
            });

            // Also for slideshow images to center properly
            document.querySelectorAll('.slideshow-block img.slide').forEach(img => {
                img.onload = () => {
                    if (img.naturalWidth < img.naturalHeight) {
                        img.style.objectFit = 'contain';
                    } else {
                        img.style.objectFit = 'cover';
                    }
                };
            });
        }

        // Slideshow logic
        function startSlideshow(container) {
            const slides = container.querySelectorAll('.slide');
            let index = 0;

            setInterval(() => {
                slides[index].classList.remove('active');
                index = (index + 1) % slides.length;

                const nextSlide = slides[index];
                if (!nextSlide.src && nextSlide.dataset.src) {
                    nextSlide.src = nextSlide.dataset.src;
                }

                nextSlide.classList.add('active');
            }, 4000);
        }


        document.querySelectorAll('.slideshow-block').forEach(block => {
            startSlideshow(block);
            block.addEventListener('click', () => {
                document.querySelectorAll('section.portfolio-section').forEach(s => s.classList.remove('active'));
                const section = document.getElementById(block.dataset.target);
                section.classList.add('active');
                section.scrollIntoView({ behavior: 'smooth' });

                // Update URL hash for deep linking
                history.pushState(null, null, `#${block.dataset.target}`);
            });
        });

        // Deep link on page load
        window.addEventListener('DOMContentLoaded', () => {
            const hash = window.location.hash.substring(1);
            if (hash) {
                const section = document.getElementById(hash);
                if (section) {
                    document.querySelectorAll('section.portfolio-section').forEach(s => s.classList.remove('active'));
                    section.classList.add('active');
                    section.scrollIntoView({ behavior: 'smooth' });
                }
            }
            detectOrientation();
        });

        // Back to Top Button
        const backToTop = document.getElementById("backToTop");
        window.onscroll = function () {
            backToTop.style.display = (document.documentElement.scrollTop > 300) ? "block" : "none";
        };

        function scrollToTop() {
            window.scrollTo({ top: 0, behavior: 'smooth' });
            // Remove hash from URL without reloading
            history.pushState(null, null, window.location.pathname);
        }
    </script>

</body>

</html>
